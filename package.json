  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@pumped-fn/lite": "1.11.2",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "puppeteer": "^24.34.0"
  }
}
